file = {::;:Users/alicearcury-quandt/Library/Application Support/Mendeley Desktop/Downloaded/Zhou et al. - 2020 - A novel bat coronavirus reveals natural insertions at the S1S2 cleavage site of the Spike protein and a possible re.pdf:pdf},
journal = {bioRxiv},
keywords = {biorxiv,coronavirus,reviewed},
mendeley-tags = {biorxiv,coronavirus,reviewed},
month = {mar},
pages = {2020.03.02.974139},
publisher = {Cold Spring Harbor Laboratory},
title = {{A novel bat coronavirus reveals natural insertions at the S1/S2 cleavage site of the Spike protein and a possible recombinant origin of HCoV-19}},
year = {2020}
}
