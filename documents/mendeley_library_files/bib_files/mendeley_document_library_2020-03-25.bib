journal = {bioRxiv},
keywords = {biorxiv,coronavirus,reviewed},
mendeley-tags = {biorxiv,coronavirus,reviewed},
month = {mar},
pages = {2020.03.04.976662},
publisher = {Cold Spring Harbor Laboratory},
title = {{Genome-wide data inferring the evolution and population demography of the novel pneumonia coronavirus (SARS-CoV-2)}},
year = {2020}
}
