issn = {1537-6591},
journal = {Clinical infectious diseases : an official publication of the Infectious Diseases Society of America},
keywords = {COVID-19,SARS-CoV-2,coronavirus,intra-host variant,microbiota,reviewed,transmission},
language = {eng},
