file = {::},
journal = {arXiv},
keywords = {coronavirus,reviewed},
mendeley-tags = {coronavirus,reviewed},
month = {mar},
title = {{Spatiotemporal fluctuation scaling law and metapopulation modeling of the novel coronavirus (COVID-19) and SARS outbreaks}},
